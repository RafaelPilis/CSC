{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}


